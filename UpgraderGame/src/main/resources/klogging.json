{
  "sinks": {
    "stdout": {
      "renderWith": "RENDER_ANSI",
      "sendTo": "STDOUT"
    }
  },
  "logging": [
    {
      "levelRanges": [
        {
          "fromMinLevel": "TRACE",
          "stopOnMatch": true,
          "toSinks": [
            "stdout"
          ]
        }
      ]
    }
  ]
}